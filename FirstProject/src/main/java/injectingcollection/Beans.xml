<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="javaCollection" class="injectingcollection.JavaCollection">
		<property name="addressList">
			<list>
				<value>Bulgaria</value>
				<value>Greec</value>
				<value>Serbia</value>
				<value>Serbia</value>
				<value>Russia</value>
				<ref bean="address1"/>
				<ref bean="address2"/>
				<ref bean="address1"/>
			</list>
		</property>
		<property name="addressSet">
			<set>
				<value>Bulgaria</value>
				<value>Greec</value>
				<value>Serbia</value>
				<value>Serbia</value>
				<value>Russia</value>
				<ref bean="address1"/>
				<ref bean="address2"/>
				<ref bean="address1"/>
			</set>
		</property>
		<property name="addressMap">
			<map>
				<entry key="1" value="Bulgaria"/>
				<entry key="2" value="Greec"/>
				<entry key="3" value="Serbia"/>
				<entry key="4" value="Serbia"/>
				<entry key="5" value="Russia"/>
				<entry key="6" value-ref="address1"/>
				<entry key="7" value-ref="address2"/>
				<entry key="8" value-ref="address1"/>
			</map>
		</property>
		<property name="addressProp">
			<props>
				<prop key="one">Bulgaria</prop>
				<prop key="two">Greec</prop>
				<prop key="three">Serbia</prop>
				<prop key="four">Serbia</prop>
				<prop key="five">Russia</prop>
			</props>
		</property>
	</bean>
	<bean id="address1" class="injectingcollection.Bean">
		<property name="country" value="USA"/>
	</bean>
	<bean id="address2" class="injectingcollection.Bean">
		<property name="country" value="England"/>
	</bean>
</beans>
